        <nlp-taxonomy:superFieldOf rdf:resource="http://www.semanticweb.org/nlp-taxonomy.owl#CommonsenseReasoning"/>
        <nlp-taxonomy:superFieldOf rdf:resource="http://www.semanticweb.org/nlp-taxonomy.owl#KnowledgeGraphReasoning"/>
