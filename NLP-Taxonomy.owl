    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/nlp-taxonomy.owl#QuestionAnswering -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/nlp-taxonomy.owl#QuestionAnswering">
